<odoo>
    <data>
        <record id="action_manpower_request" model="ir.actions.report">
            <field name="name">Print Manpower Request</field>
            <field name="model">manpower.request</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">custom_report.template_report_manpower_request</field>
            <field name="report_file">custom_report.template_report_manpower_request</field>
            <field name="print_report_name">'%s' % (object.name)</field>
        </record>
        <template id="template_report_manpower_request">
            <t t-call="web.html_container">
                <t t-set="doc" t-value="docs"/>
                <t t-call="web.external_layout">TEST</t>
            </t>
        </template>
    </data>
</odoo>