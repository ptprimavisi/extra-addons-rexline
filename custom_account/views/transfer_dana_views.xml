<odoo>
    <data>
        <record id="transfer_dana_form_view" model="ir.ui.view">
            <field name="name">transfer.dana.wizard.form.view</field>
            <field name="model">transfer.dana.wizard</field>
                <field name="arch" type="xml">
                    <form>
                        <group>
                            <group>
<!--                                <field name="name"/>-->
                                <field name="date_transfer" string="Transfer Date"/>
                                <field name="amount"/>
                            </group>
                        </group>
                        <footer>
                            <button name="action_transfer" string="Transfer" type="object"/>
                        </footer>
                    </form>
                </field>
        </record>

        <record id="seq_transfer_dana" model="ir.sequence">
            <field name="name">Transfer dana</field>
            <field name="code">TRANS</field>
            <field name="prefix">TRANSFER/%(y)s/000</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
