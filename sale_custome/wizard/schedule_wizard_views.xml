<odoo>
    <data>
        <record id="schedule_wizard_form_view" model="ir.ui.view">
            <field name="name">schedule.wizard.view.form</field>
            <field name="model">schedule.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="date_from" string="Date From" required="1"/>
                            </group>
                            <group>
                                <field name="date_to" string="Date To" required="1"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button
                            name="button_save"
                            string="Confirm"
                            type="object"
                            class="oe_highlight"
                        />
                    </footer>
                </form>
            </field>
        </record>

